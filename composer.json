{
  "name": "paksuco/backend",
  "description": "WIP - Admin template package",
  "keywords": [
    "laravel",
    "admin",
    "template",
    "package"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Taha PAKSU",
      "email": "tpaksu@gmail.com",
      "homepage": "https://rettica.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "^7.0",
    "illuminate/config": "^7.0",
    "livewire/livewire": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Paksuco\\Backend\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Paksuco\\Backend\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Paksuco\\Backend\\BackendServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}